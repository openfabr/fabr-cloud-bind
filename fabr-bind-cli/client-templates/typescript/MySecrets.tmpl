import { Secrets } from "./libs/Secrets";

// This is an example design of a class that will be generated from the fabr.outputs.json file.

export class MySecrets extends Secrets {
  {{#each params}}
    {{#if this.isSecret}}
      private _{{@key}}: string | undefined;
      async {{@key}}() {
        if (!this._{{@key}}) {
          this._{{@key}} = await this.getSecret("{{this.value}}")
        }
        return this._{{@key}};    
      }
    {{/if}}
  {{/each}}
}
